{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "serve-dev": "tsc && vite build --mode development --config vite.config.js && webpack --config webpack.config.cjs && vite",
    "serve-prod": "tsc && vite build --mode production --config vite.config.js && webpack --config webpack.config.cjs && vite preview",
    "serve": "vite",
    "build": "tsc && vite build --mode production --config vite.config.js && webpack --config webpack.config.cjs",
    "preview": "vite preview",
    "webpack": "webpack --config webpack.config.cjs",
    "watcher": "node compile-component-sass.cjs"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-loading-skeleton": "^3.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "chokidar": "^3.5.3",
    "css-loader": "^6.8.1",
    "date-fns": "^2.30.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.24",
    "postcss-loader": "^7.3.3",
    "sass": "^1.64.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  }
}
